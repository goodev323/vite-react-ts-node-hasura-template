{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "graphql:start": "docker-compose -f ../docker-compose.yaml --env-file ../.env.local up -d",
    "graphql:stop": "docker-compose -f ../docker-compose.yaml --env-file ../.env.local down",
    "graphql:restart": "docker-compose -f ../docker-compose.yaml --env-file ../.env.local restart",
    "graphql:codegen": "DOTENV_CONFIG_PATH=../.env.local graphql-codegen -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "graphql": "^16.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "hasura-cli": "^2.2.0",
    "prettier": "^2.6.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
